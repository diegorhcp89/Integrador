<%- include('./partials/head'); %>
    <%- include('./partials/header'); %>

    <section>

        <div class="group">
        <div class="text-center">
            <form>
                <img class="mt-4 mb-4" src="/images/login.png" height="150" alt="login-logo"></a>
                <h1>login do cliente</h1>
                <label for="emailAddress" class="sr-only">Endereço de E-mail</label>
                <input type="email" id="emailAddress" class="form-control" placeholder="Endereço de E-mail" required autofocus>
                <label for="password" class="sr-only">Senha</label>
                <input type="password" id="password" placeholder="Senha" class="form-control">
                <div class="checkbox mt-3">
                    <label>
                        <input type="checkbox" value="remember-me">
                        <span>Lembrar-me</span> 
                    </label>
                </div>
                <div class="mt-3">
                    <button class="btn btn-lg btn-primary btn-block">Continuar</button>
                </div>
                <div class="medias">
                    <span>Ou conecte com sua rede social</span>
                    <div class="social-fields">
                        <div class="social-field google">
                            <a href="#">
                            <i class="fab fa-google"></i>
                            Login com conta Google
                            </a>
                        </div>
                    </div>
                    <div class="social-fields">
                        <div class="social-field facebook">
                            <a href="#">
                            <i class="fab fa-facebook-f"></i>
                            Login com conta Facebook
                            </a>
                        </div>
                    </div>
                    <div class="mt-3">
                        <span>não tem cadastro? <a href="#">cadastre-se</a></span>
                    </div>
                </div>
            </form>
        </div>
        </div>

    </section>


    <%- include('./partials/footer'); %>
</body>

</html>